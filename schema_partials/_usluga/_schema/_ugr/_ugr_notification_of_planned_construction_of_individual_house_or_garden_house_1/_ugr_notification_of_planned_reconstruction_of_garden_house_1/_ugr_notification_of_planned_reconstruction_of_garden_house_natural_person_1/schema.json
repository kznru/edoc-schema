{
  "type": "object",
  "properties": {
    "agent": {
      "$ref": "http://edoc-schema.kzn.ru/person/ru/agent",
      "title": "Заявитель",
      "description": "ФИО Застройщика и Правообладателя земельного участка"
    },
    "applicant_info": {
      "type": "object",
      "title": "Данные заявителя",
      "properties": {
        "registration_address": {
          "$ref": "http://edoc-schema.kzn.ru/location/ru/address",
          "title": "Место жительства",
          "description": "Адрес проживания"
        },
        "passport": {
          "type": "object",
          "title": "Реквизиты документа, удостоверяющего личность",
          "required": ["seria", "issued_by", "issue_date"],
          "properties": {
            "seria": {
              "type": "string",
              "title": "Серия"
            },
            "issued_by": {
              "type": "string",
              "title": "Кем выдан"
            },
            "issue_date": {
              "type": "string",
              "format": "date",
              "title": "Дата выдачи"
            }
          }
        }
      }
    }
  }
}
