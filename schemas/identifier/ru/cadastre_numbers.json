{
    "id": "http://edoc-schema.kzn.ru/identifier/ru/cadastre_numbers",
    "$schema": "http://json-schema.org/draft-06/schema#",
    "description": "Кадастровые номера",
    "title": "Кадастровые номера",
    "type": "array",
    "minItems": 1,
    "items": {
      "type": "string",
      "pattern": "^\\d+([:/-]\\d+){3,}$"
    }
}
