{
  "id": "http://edoc-schema.kzn.ru/document/ru/uslugi/definitions/borders_schemas",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "Схема расположения земельного участка на кадастровом плане территории",
  "type": "object",
  "required":[
    "format_xml",
    "format_html",
    "format_pdf"
  ],
  "properties":{
    "format_xml": {
      "$ref": "http://edoc-schema.kzn.ru/document/ru/uslugi/definitions/scanned_document",
      "accept": "application/xml, text/xml",
      "title": "Формат XML"
    },
    "format_html": {
      "$ref": "http://edoc-schema.kzn.ru/document/ru/uslugi/definitions/scanned_document",
      "accept": "text/html",
      "title": "Формат HTML"
    },
    "format_pdf": {
      "$ref": "http://edoc-schema.kzn.ru/document/ru/uslugi/definitions/scanned_document",
      "accept": "application/pdf",
      "title": "Формат PDF"
    }
  }
}